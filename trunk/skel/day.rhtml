<%= @vil.vid %> <%= @vil.name %>

<p>
<% timeline = @vil.timeline; timeline.each_with_index {|d, index| %>
<% if index == @date %>
<span class="time"><%= d %></span> 
<% elsif index == @vil.date %>
<a href="./?vid=<%= @vil.vid %>"><%= d %></a> 
<% else %>
<a href="./?vid=<%= @vil.vid %>;date=<%= index.to_s %>"><%= d %></a> 
<% end %>
<% } %>
</p>

<div id="discuss">
<%= @vil.discussions(@date, @player).join %>
